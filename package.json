{
  "name": "charon",
  "description": "A node.js API client factory to abstract away some of the nitty gritty aspects of interacting with a ReST API over HTTP.",
  "tags": [
    "REST",
    "api",
    "client"
  ],
  "version": "0.9.0",
  "author": "Sam Placette <sam@samplacette.com>",
  "repository": {
    "type": "git",
    "url": "http://github.com/SamPlacette/charon.git"
  },
  "bugs": {
    "url": "http://github.com/SamPlacette/charon/issues"
  },
  "engines": [
    "node >= 0.8.0"
  ],
  "main": "index.js",
  "dependencies": {
    "request": "~2.27.0",
    "underscore": "~1.5.2",
    "async": "~0.2.9"
  },
  "devDependencies": {
    "instanbul": "*",
    "mocha": "*",
    "nock": "*",
    "should": "*",
    "sinon": "*",
    "jsdoc": "*"
  },
  "scripts": {
    "test": "node node_modules/.bin/mocha tests/",
    "test-cov": "./node_modules/istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000 tests/",
    "jsdoc": "rm -r out/; jsdoc index.js; echo \"file://`pwd`/out/index.html\""
  }
}
